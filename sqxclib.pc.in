prefix=@SQXCLIB_INSTALL_PREFIX@
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: sqxclib
Description: sqxclib is a library to convert SQL (or JSON...etc) data to/from C language.
Version: @SQXCLIB_VERSION@
# Requires: json-c sqlite mysql
Cflags: -I${includedir}/sqxc -I${includedir}/sqxc/app -I${includedir}/sqxc/support
Libs: -L${libdir} -lsqxcapptool -lsqxcsupport -lsqxcapp -lsqxc
